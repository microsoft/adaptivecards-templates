{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ],
      "route": "{*name}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "blob",
      "name": "templateBlobLegacy",
      "path": "templates/{name}",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "type": "blob",
      "name": "templateBlob",
      "path": "templates/{name}.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "type": "blob",
      "name": "templateBlobData",
      "path": "templates/{name}.data.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "type": "blob",
      "name": "templatesBlob",
      "path": "templates/templates.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    }
  ],
  "scriptFile": "../dist/GetTemplate/index.js"
}

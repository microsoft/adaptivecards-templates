{
  "name": "private-templates-service",
  "version": "1.0.0",
  "description": "Collaboration tool simplifying the workflow of developing adaptive cards.",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start:prod": "npm install && node server.js",
    "start": "tsc && node dist/server.js | bunyan",
    "build": "tsc",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/server.ts | bunyan",
    "client": "cd ../client && npm start"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "^3.1.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "connect-mongo": "^3.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.3.1",
    "jws": "^4.0.0",
    "mongoose": "^5.8.9",
    "passport": "^0.4.1",
    "passport-azure-ad": "^4.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^9.4.0",
    "@types/async": "^3.0.5",
    "@types/bluebird": "^3.5.29",
    "@types/body-parser": "^1.17.1",
    "@types/bunyan": "^1.8.6",
    "@types/connect-mongo": "^3.0.0",
    "@types/express-validator": "^3.0.0",
    "@types/jws": "^3.2.1",
    "@types/mongoose": "^5.5.43",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.0",
    "@types/passport": "^1.0.2",
    "@types/passport-azure-ad": "^4.0.3",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2",
    "jest": "^25.1.0",
    "jest-dom": "^4.0.0",
    "office-ui-fabric-react": "^7.85.0",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5"
  }
}
